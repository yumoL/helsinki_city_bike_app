{
  "apps": {
    "name": "HSL-city-bike-backend",
    "script": "bin/www",
    "watch": true,
    "ignore_watch": [
      "node_modules",
      "logs",
      "uploadedFiles"
    ],
    "instances": 4,
    "error_file": "./logs/err.log",
    "out_file": "./logs/out.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "env": {
      "NODE_ENV": "production"
    }
  }
}